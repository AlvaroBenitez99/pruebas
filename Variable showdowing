const precioRosa=8;
let cantidadRosas=70;
let totalRosas=precioRosa*cantidadRosas;
const precioLilio=10;
let cantidadLilio=50;
let totalLilio=precioLilio*cantidadLilio;
const precioTulipan=2;
let cantidadTulipan=120;
let totalTulipan=precioTulipan*cantidadTulipan;
let totalFlores=totalLilio+totalTulipan+totalRosas;
console.log("Rose - unit price ",precioRosa,", Cantidad Rosas  ", cantidadRosas, "Precio total Rosas ",totalRosas );
console.log(totalFlores);
cantidadRosas=30;
cantidadLilio=20;
totalRosas=precioRosa*cantidadRosas;
totalLilio=precioLilio*cantidadLilio;
totalFlores=totalLilio+totalTulipan+totalRosas;
console.log("Rose - unit price ",precioRosa,", Cantidad Rosas  ", cantidadRosas, "Precio total Rosas ",totalRosas );
console.log(totalFlores);
